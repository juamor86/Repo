<?xml version="1.0" encoding="utf-8"?>
<es.juntadeandalucia.msspa.saludandalucia.presentation.dynamic.DynamicGridLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="match_parent">

    <LinearLayout
        android:id="@+id/left_top_container_ll"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:layout_marginEnd="@dimen/margin_default"
        android:layout_marginBottom="@dimen/margin_default"
        android:background="@drawable/sh_rounded_rect"
        android:orientation="vertical"
        app:layout_constraintBottom_toTopOf="@id/horizontal_75_gl"
        app:layout_constraintEnd_toStartOf="@id/vertical_50_gl"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintDimensionRatio="364:435"/>

    <LinearLayout
        android:id="@+id/left_bottom_container_ll"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:layout_marginEnd="@dimen/margin_default"
        android:background="@drawable/sh_rounded_rect"
        android:orientation="vertical"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toStartOf="@id/vertical_50_gl"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="@id/horizontal_75_gl"
        app:layout_constraintDimensionRatio="910:478"/>


    <LinearLayout
        android:id="@+id/right_top_container_ll"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:layout_marginBottom="@dimen/margin_default"
        android:background="@drawable/sh_rounded_rect"
        android:orientation="vertical"
        app:layout_constraintBottom_toTopOf="@id/right_bottom_container_ll"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toEndOf="@id/vertical_50_gl"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintDimensionRatio="128:157">

    </LinearLayout>

    <LinearLayout
        android:id="@+id/right_bottom_container_ll"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:background="@drawable/sh_rounded_rect"
        android:orientation="vertical"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toEndOf="@id/vertical_50_gl"
        app:layout_constraintTop_toBottomOf="@id/right_top_container_ll"
        app:layout_constraintDimensionRatio="128:157">

    </LinearLayout>

    <androidx.constraintlayout.widget.Guideline
        android:id="@+id/vertical_50_gl"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="vertical"
        app:layout_constraintGuide_percent="0.5975" />

    <androidx.constraintlayout.widget.Guideline
        android:id="@+id/horizontal_75_gl"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="horizontal"
        app:layout_constraintGuide_percent="0.7023" />

</es.juntadeandalucia.msspa.saludandalucia.presentation.dynamic.DynamicGridLayout>